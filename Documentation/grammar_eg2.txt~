DEVICES
{
	CLOCK -period 5 = clock;
	SWITCH -initialvalue 0 = switch1;
	SWITCH -initialvalue 1 = switch2;
	SWITCH -initialvalue 1 = switch3;
	DTYPE = dtype;
}
CONNECTIONS
{
	switch3 > dtype.DATA;
		switch1 > dtype.DATA;
	switch1 > dtype.SET;
	switch2 > dtype.CLEAR;
	clock > dtype.CLK;
}
MONITOR
{
	dtype.Q;
	dtype.QBAR;
	clock;
}
